{
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true
    },
    "plugins": ["jest"],
    "extends": ["airbnb", "plugin:react/jsx-runtime"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/prop-types": "off",
        "import/prefer-default-export": "off",
        "import/no-named-as-default": "off"
    }
}
